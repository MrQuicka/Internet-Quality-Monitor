{
  "title": "Internet Quality Monitor",
  "schemaVersion": 38,
  "editable": true,
  "time": { "from": "now-24h", "to": "now" },
  "timezone": "browser",
  "panels": [
    {
      "type": "stat",
      "title": "Download (Mb/s)",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
      "targets": [{ "expr": "iqm_download_mbps" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } }
    },
    {
      "type": "stat",
      "title": "Upload (Mb/s)",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
      "targets": [{ "expr": "iqm_upload_mbps" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } }
    },
    {
      "type": "stat",
      "title": "Ping (ms)",
      "gridPos": { "x": 12, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "iqm_ping_ms" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Jitter (ms)",
      "gridPos": { "x": 16, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "iqm_jitter_ms" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Packet loss (%)",
      "gridPos": { "x": 20, "y": 0, "w": 4, "h": 4 },
      "targets": [{ "expr": "iqm_packet_loss_pct" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "timeseries",
      "title": "Download / Upload (Mb/s)",
      "gridPos": { "x": 0, "y": 4, "w": 24, "h": 9 },
      "targets": [
        { "expr": "iqm_download_mbps", "legendFormat": "download" },
        { "expr": "iqm_upload_mbps", "legendFormat": "upload" }
      ],
      "options": { "legend": { "displayMode": "list", "placement": "right" } }
    },
    {
      "type": "timeseries",
      "title": "Ping / Jitter / Loss",
      "gridPos": { "x": 0, "y": 13, "w": 24, "h": 9 },
      "targets": [
        { "expr": "iqm_ping_ms", "legendFormat": "ping" },
        { "expr": "iqm_jitter_ms", "legendFormat": "jitter" },
        { "expr": "iqm_packet_loss_pct", "legendFormat": "loss (%)" }
      ],
      "options": { "legend": { "displayMode": "list", "placement": "right" } }
    }
  ],
  "templating": {
    "list": []
  },
  "annotations": { "list": [] },
  "version": 1
}
